{
  "arch": "wasm32",
  "crt-objects-fallback": "wasm",
  "data-layout": "e-m:e-p:32:32-i64:64-n32:64-S128-ni:1:10:20",
  "default-adjusted-cabi": "wasm",
  "dll-prefix": "",
  "dll-suffix": ".wasm",
  "default-hidden-visibility": true,
  "dynamic-linking": true,
  "eh-frame-header": false,
  "emit-debug-gdb-scripts": false,
  "exe-suffix": ".app",
  "executables": true,
  "has-elf-tls": true,
  "is-builtin": false,
  "is-like-wasm": true,
  "limit-rdylib-exports": false,
  "linker": "rust-lld",
  "linker-flavor": "wasm-ld",
  "linker-is-gnu": false,
  "lld-flavor": "wasm",
  "llvm-target": "wasm32-ardaku",
  "max-atomic-width": 64,
  "only-cdylib": true,
  "os": "ardaku",
  "panic-strategy": "abort",
  "main-needs-argc-argv": false,
  "relocation-model": "static",
  "singlethread": false,
  "target-family": ["wasm"],
  "target-pointer-width": "32",
  "tls-model": "local-exec",
  "pre-link-args": {
    "gcc": [
      "-Wl,-z",
      "-Wl,stack-size=1048576",
      "-Wl,--stack-first",
      "-Wl,--allow-undefined",
      "-Wl,--fatal-warnings",
      "-Wl,--no-demangle",
      "--target=wasm32-ardaku",
      "-Wl,--no-entry",
      "-Wl,--export-dynamic"
    ],
    "wasm-ld": [
      "-z",
      "stack-size=1048576",
      "--stack-first",
      "--allow-undefined",
      "--fatal-warnings",
      "--no-demangle",
      "--no-entry",
      "--export-dynamic"
    ]
  }
}
